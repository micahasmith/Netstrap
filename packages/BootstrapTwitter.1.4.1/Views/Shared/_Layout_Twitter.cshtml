<!DOCTYPE html>
<html>
<head>
   <meta charset="utf-8" />
   <title>@ViewBag.Title</title>
   <link href="@Url.Content("~/Content/bootstrap.css")" rel="stylesheet" type="text/css" />
   <link rel="shortcut icon" type="image/x-icon" href="http://twitter.github.com/bootstrap/assets/ico/favicon.ico">

</head>
<body>
   <div class="container" style="padding-top: 40px;">
      <section id="about">
         <div class="page-header">
            <h1>
               About Bootstrap <small>Brief history, browser support, and more</small></h1>
         </div>
         <div class="row">
            @RenderBody()
         </div>
      </section>
      <!-- /row -->
   </div>
   <footer class="footer">
      <div class="container">
         <p class="pull-right">
            <a href="https://twitter.com/fransstridh" class="twitter-follow-button" data-show-count="false">
               Follow @@fransstridh</a>
            <script src="//platform.twitter.com/widgets.js" type="text/javascript"></script>
         </p>
      </div>
   </footer>
   <script src="http://code.jquery.com/jquery-1.7.1.min.js" type="text/javascript"></script>
   <script src="../../Scripts/bootstrap-tabs.js" type="text/javascript"></script>
   <script src="../../Scripts/bootstrap-alerts.js" type="text/javascript"></script>
   <script src="../../Scripts/bootstrap-buttons.js" type="text/javascript"></script>
   <script src="../../Scripts/bootstrap-dropdown.js" type="text/javascript"></script>
   <script src="../../Scripts/bootstrap-modal.js" type="text/javascript"></script>
   <script src="../../Scripts/bootstrap-popover.js" type="text/javascript"></script>
   <script src="../../Scripts/bootstrap-scrollspy.js" type="text/javascript"></script>
   @RenderSection("_javascript", false)
</body>
</html>
